<!doctype html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, user-scalable=yes, target-densitydpi=160">
  </head>
  <body>
    <div class="container">
      {{#times 50}}
        {{> panel }}
      {{/times}}
      <div class="content">
        <h3>
          <span>An LGBTQ+ Comedy Night</span>
        </h3>
      </div>
      <div class="bar bar--top">
        <div class="bar__inner">
          <p>Every other Monday at Moodring</p>
        </div>
      </div>
      <div class="bar bar--bottom">
        <div class="bar__inner">
          <p>Hosted by Sam Campbell, Simone Leitner, and Peter Valenti</p>
        </div>
      </div>
    </div>
  </body>
</html>